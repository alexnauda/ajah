<?xml version="1.0" encoding="UTF-8"?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" version="2.0">
	<tlib-version>1.0</tlib-version>
	<short-name>asm</short-name>
	<uri>http://ajah.com/spring-mvc</uri>

	<tag>
		<name>form</name>
		<tag-class>com.ajah.spring.mvc.servlet.tag.AutoFormTag</tag-class>
		<attribute>
			<name>autoForm</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

</taglib>